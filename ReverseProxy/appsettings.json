{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "adminSwagger": {
        "ClusterId": "adminCluster",
        "Match": { "Path": "/api/admin/swagger/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/api/admin" } ],
        "Order": 0
      },
      "consumerSwagger": {
        "ClusterId": "consumerCluster",
        "Match": { "Path": "/api/consumer/swagger/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/api/consumer" } ],
        "Order": 0
      },
      "providerSwagger": {
        "ClusterId": "providerCluster",
        "Match": { "Path": "/api/provider/swagger/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/api/provider" } ],
        "Order": 0
      },
      "adminRoute": {
        "ClusterId": "adminCluster",
        "Match": { "Path": "/api/admin/{**catch-all}" },
        "Order": 10
      },
      "consumerRoute": {
        "ClusterId": "consumerCluster",
        "Match": { "Path": "/api/consumer/{**catch-all}" },
        "Order": 10
      },
      "providerRoute": {
        "ClusterId": "providerCluster",
        "Match": { "Path": "/api/provider/{**catch-all}" },
        "Order": 10
      }
    },
    "Clusters": {
      "adminCluster": {
        "Destinations": {
          "adminDestination": { "Address": "http://adminservice:8080" }
        }
      },
      "consumerCluster": {
        "Destinations": {
          "consumerDestination": { "Address": "http://consumerservice:8080" }
        }
      },
      "providerCluster": {
        "Destinations": {
          "providerDestination": { "Address": "http://providerservice:8080" }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
